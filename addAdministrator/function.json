{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "programme",
      "databaseName": "Data",
      "collectionName": "Programme",
      "connectionStringSetting": "progmandb_DOCUMENTDB",
      "id": "{Query.programme}",
      "partitionKey": "{Query.u1}"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "u1",
      "databaseName": "Data",
      "collectionName": "User",
      "connectionStringSetting": "progmandb_DOCUMENTDB",
      "id": "{Query.u1}",
      "partitionKey": "{Query.university}"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "u2",
      "databaseName": "Data",
      "collectionName": "User",
      "connectionStringSetting": "progmandb_DOCUMENTDB",
      "id": "{Query.u2}",
      "partitionKey": "{Query.university}"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "outputDocument",
      "databaseName": "Data",
      "collectionName": "Programme",
      "createIfNotExists": "false",
      "connectionStringSetting": "progmandb_DOCUMENTDB"
    }
  ]
}
